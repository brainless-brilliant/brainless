{
  "name": "@brainless-brilliant/brainless",
  "version": "2.2.0",
  "description": "AI Workforce - Autonomous multi-agent orchestration with PM-managed team execution, inter-agent debate, and full activity transcripts.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "brainless": "dist/cli/index.js",
    "bl": "dist/cli/index.js",
    "bl-analytics": "dist/cli/analytics.js"
  },
  "files": [
    "dist",
    "agents",
    "bridge",
    "commands",
    "hooks",
    "scripts",
    "skills",
    "docs",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.ts",
    "sync-metadata": "tsx scripts/sync-metadata.ts",
    "sync-metadata:verify": "tsx scripts/sync-metadata.ts --verify",
    "sync-metadata:dry-run": "tsx scripts/sync-metadata.ts --dry-run",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "@anthropic-ai/sdk": "^0.72.1",
    "@ast-grep/napi": "^0.31.0",
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "jsonc-parser": "^3.3.1",
    "uuid": "^13.0.0",
    "vscode-languageserver-protocol": "^3.17.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.19.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/ui": "^4.0.17",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brainless-brilliant/brainless.git"
  },
  "homepage": "https://github.com/brainless-brilliant/brainless",
  "bugs": {
    "url": "https://github.com/brainless-brilliant/brainless/issues"
  },
  "author": {
    "name": "brainless-brilliant",
    "email": "atmanbhatt7@gmail.com",
    "url": "https://github.com/brainless-brilliant"
  },
  "contributors": [
    {
      "name": "Yeochan Heo",
      "url": "https://github.com/yeochanheo",
      "note": "Original author of oh-my-claudecode - the foundation this project builds upon"
    },
    {
      "name": "brainless-brilliant",
      "url": "https://github.com/brainless-brilliant",
      "note": "Extended with autonomous team orchestration, PM agents, debate protocol, and activity transcripts"
    }
  ],
  "license": "MIT",
  "keywords": [
    "brainless",
    "ai-workforce",
    "claude",
    "claude-code",
    "ai",
    "agent",
    "multi-agent",
    "orchestration",
    "workforce-provisioning",
    "autonomous-agents",
    "pm-agent",
    "debate-protocol",
    "anthropic",
    "llm"
  ],
  "publishConfig": {
    "access": "public"
  }
}
